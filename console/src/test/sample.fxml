<GridPane fx:id="grid" alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121" fx:controller="test.Controller">
   <CheckBox mnemonicParsing="false" text="CheckBox" />
